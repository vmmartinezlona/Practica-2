<head>
   <meta charset = "UTF-8">
   <title>Welcome</title>
   <link rel = "stylesheet" href = "/stylesheets/employeeList.css">
</head>

<body>

  <img class="header" src="/images/header.png">

  <div class = "toolbar">
    <label><input type "text" class = "input-search" placeholder = "Search"></label>
    <label><button> Search </button></label>
    <br>
    <button type="submit" onclick="location.href='/employee/add'"> Add Employee </button>
  </div>

  <div class = "employeecontainer">
    <br>
    {{#each employee}}
    {{this.lastName}} {{this.firstName}}
    <img src="{{this.photo}}" class="img-responsive" alt="{{this.lastName}}">
    {{/each}}
  </div>

</body>
</html>
